    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch TutorialASA.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/TutorialASA.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch CreateNewAccounts.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/CreateNewAccounts.js"
        },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch myTutorialASA.js",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/myTutorialASA.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch my myCreateNewAccounts.js",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/myCreateNewAccounts.js"
    }       

    ]
}